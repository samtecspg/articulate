{
  "name": "articulate",
  "version": "1.0.0",
  "description": "Smart conversational interfaces",
  "repository": {
    "type": "git",
    "url": "git://github.com/samtecspg/articulate.git"
  },
  "engines": {
    "npm": ">=5",
    "node": ">=10.5.0"
  },
  "author": "Samtec Smart Platform Group",
  "license": "Apache-2.0",
  "main": "server/index.js",
  "scripts": {
    "analyze:clean": "rimraf stats.json",
    "preanalyze": "npm run analyze:clean",
    "analyze": "node ./internals/scripts/analyze.js",
    "extract-intl": "node ./internals/scripts/extract-intl.js",
    "npmcheckversion": "node ./internals/scripts/npmcheckversion.js",
    "preinstall": "npm run npmcheckversion",
    "prebuild": "npm run build:clean",
    "build": "cross-env NODE_ENV=production env-cmd --no-override .env webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build:clean": "rimraf ./build",
    "start": "cross-env NODE_ENV=development env-cmd --no-override .env node server",
    "start:tunnel": "cross-env NODE_ENV=development env-cmd --no-override .env ENABLE_TUNNEL=true node server",
    "start:production": "npm run test && npm run build && npm run start:prod",
    "start:prod": "cross-env NODE_ENV=production env-cmd --no-override .env node server",
    "start:debug": "npm start --inspect",
    "presetup": "npm i chalk shelljs",
    "setup": "node ./internals/scripts/setup.js",
    "clean": "shjs ./internals/scripts/clean.js",
    "clean:all": "npm run analyze:clean && npm run test:clean && npm run build:clean",
    "generate": "plop --plopfile internals/generators/index.js",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "stylelint app/**/*.js",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts .",
    "lint:eslint:fix": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts --fix .",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:staged": "lint-staged",
    "pretest": "npm run test:clean && npm run lint",
    "test:clean": "rimraf ./coverage",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "prettify": "prettier --write \"{app,server,internals}/**/*.{js,jsx,json}\""
  },
  "keywords": [],
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "IE 10"
  ],
  "lint-staged": {
    "*.js": [
      "npm run lint:eslint:fix",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "pre-commit": "lint:staged",
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "dependencies": {
    "@babel/polyfill": "7.4.3",
    "@material-ui/core": "3.6.1",
    "@material-ui/lab": "3.0.0-alpha.24",
    "axios": "^0.18.0",
    "chalk": "2.4.2",
    "chart.js": "^2.8.0",
    "compression": "1.7.4",
    "connected-react-router": "6.4.0",
    "font-color-contrast": "^1.0.3",
    "guid": "^0.0.12",
    "history": "4.9.0",
    "hoist-non-react-statics": "3.3.0",
    "immer": "8.0.1",
    "intl": "1.2.5",
    "invariant": "2.2.4",
    "ip": "1.1.5",
    "javascript-time-ago": "2.0.1",
    "lodash": "^4.17.19",
    "markdown-to-jsx": "^6.11.4",
    "material-colors": "1.2.6",
    "minimist": "1.2.3",
    "moment-range": "^4.0.2",
    "nes": "10.0.2",
    "prop-types": "15.7.2",
    "query-string": "6.2.0",
    "react": "16.8.6",
    "react-ace": "6.1.4",
    "react-audio-player": "^0.11.1",
    "react-autosuggest": "9.4.2",
    "react-chartjs-2": "^2.7.6",
    "react-color": "2.14.1",
    "react-contenteditable": "^3.2.6",
    "react-dnd": "7.0.2",
    "react-dnd-html5-backend": "7.0.2",
    "react-dom": "16.8.6",
    "react-hot-loader": "^4.12.18",
    "react-intl": "2.9.0",
    "react-loadable": "5.4.0",
    "react-redux": "7.0.2",
    "react-router-dom": "5.0.0",
    "react-router-redux": "^4.0.8",
    "redux": "4.0.1",
    "redux-saga": "1.0.2",
    "redux-seamless-immutable": "0.4.0",
    "reselect": "4.0.0",
