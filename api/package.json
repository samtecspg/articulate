{
  "name": "alpha-nlu-api",
  "version": "0.0.1",
  "description": "NLU API",
  "main": "start.js",
  "scripts": {
    "test": "lab -v --lint --leaks",
    "lint": "lab -dL",
    "lint:fix": "lab -dL --lint-fix",
    "coverage": "lab -dc",
    "test:full": "lab --lint --coverage",
    "start": "npm-run-all --parallel test start:server",
    "start:server": "nodemon start.js",
    "precommit": "npm run git:hook",
    "prepush": "npm run git:hook",
    "git:hook": "lab --lint --leaks"
  },
  "keywords": [],
  "author": "Daniel Calvo",
  "license": "SEE LICENSE IN LICENSE.md",
  "devDependencies": {
    "code": "^4.0.0",
    "husky": "^0.13.3",
    "lab": "^13.0.4",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2"
  },
  "dependencies": {
    "@slack/client": "^4.3.1",
    "async": "^2.4.0",
    "axios": "^0.18.0",
    "boom": "^4.3.1",
    "crypto": "^0.0.3",
    "dashify": "^0.2.2",
    "debug": "^2.6.7",
    "dotenv": "^4.0.0",
    "elasticsearch": "^15.1.1",
    "flat": "^4.0.0",
    "guid": "^0.0.12",
    "handlebars": "^4.0.11",
    "handlebars-helpers": "^0.10.0",
    "handlebars-intl": "^1.1.2",
    "hapi": "^16.1.1",
    "hapi-swagger": "^7.7.0",
    "hapi-swaggered-ui": "^2.6.2",
    "inert": "^4.2.0",
    "joi": "^10.4.1",
    "joi-phone-number": "^2.0.12",
    "jsesc": "^2.5.1",
    "json2xml": "^0.1.3",
    "json2yaml": "^1.1.0",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "moment-timezone": "^0.5.13",
    "numeral": "^2.0.6",
    "ordinal": "^1.0.2",
    "redis": "^2.8.0",
    "shortid": "^2.2.8",
    "twilio": "^3.17.0",
    "vision": "^4.1.1",
    "wreck": "^12.2.0"
  },
  "eslintConfig": {
    "extends": "eslint-config-hapi",
    "rules": {
      "quotes": [
        "error",
        "double"
      ]
    }
  },
  "repository": "https://github.com/samtecspg/alpha-nlu-api.git"
}
