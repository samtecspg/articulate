{
  "name": "articulate",
  "version": "1.0.0",
  "description": "Articulate API",
  "author": "Samtec Smart Platform Group",
  "license": "Apache-2.0",
  "main": "server/index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development env-cmd server/.env nodemon server --exec babel-node",
    "start:debug": "cross-env NODE_ENV=development env-cmd server/.env babel-node server $NODE_DEBUG_OPTION",
    "start:remote-debug": "cross-env NODE_ENV=development env-cmd server/.env babel-node server --inspect",
    "start:prod": "cross-env NODE_ENV=production env-cmd server/.env babel-node server",
    "test": "AUTH_ENABLED=false DEBUG=-* REDIS_HOST=0.0.0.0 ES_HOST=http://0.0.0.0:9200 lab -v -a code -T node_modules/lab-transform-preset-env -S $NODE_DEBUG_OPTION -m 10000 -l",
    "test-cov": "DEBUG=-* REDIS_HOST=0.0.0.0 ES_HOST=http://0.0.0.0:9200 lab -r html -o coverage.html",
    "coverage:html": "DEBUG=-articulate:* lab -a code -T node_modules/lab-transform-preset-env -S -c -r html -o coverage.html",
    "lint": "lab -dL -T node_modules/lab-transform-preset-env -S",
    "lint:fix": "lab -dL --lint-fix -T node_modules/lab-transform-preset-env -S"
  },
  "dependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@elastic/elasticsearch": "^7.5.0",
    "@hapi/basic": "^5.1.0",
    "@rocket.chat/sdk": "^0.2.9-2",
    "@slack/client": "^4.9.0",
    "@slack/webhook": "^5.0.0",
    "actions-on-google": "^2.6.0",
    "await-semaphore": "^0.1.3",
    "axios": "^0.18.1",
    "bell": "^10.0.0",
    "blipp": "^3.1.3",
    "boom": "^7.2.0",
    "bootbot": "^1.0.16",
    "confidence": "3.x.x",
    "cross-env": "^5.2.0",
    "debug": "^4.0.1",
    "env-cmd": "^8.0.2",
    "fast-string-search": "^1.4.1",
    "glue": "5.x.x",
    "guid": "^0.0.12",
    "handlebars": "^4.0.12",
    "handlebars-helpers": "^0.10.0",
    "handlebars-intl": "^1.1.2",
    "handlebars.moment": "^1.0.4",
    "hapi": "17.x.x",
    "hapi-auth-cookie": "9.1.0",
    "hapi-swagger": "^9.3.1",
    "hapi-swaggered": "^3.0.3",
    "hapi-swaggered-ui": "^3.0.2",
    "haute-couture": "3.x.x",
    "hoek": "^6.1.3",
    "inert": "^5.1.0",
    "joi": "13.x.x",
    "joi-phone-number": "^2.1.1",
    "jsesc": "^2.5.0",
    "json2xml": "^0.1.3",
    "json2yaml": "^1.1.0",
    "jsonpath-plus": "^0.18.0",
    "loadbalance": "^1.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.23",
    "nes": "^9.0.1",
    "nohm": "^2.2.0",
    "numeral": "^2.0.6",
    "redis": "^2.8.0",
    "require-dir": "^1.0.0",
    "schmervice": "^1.0.0",
    "toys": "2.x.x",
    "twilio": "^3.28.0",
    "vision": "^5.4.0",
    "vm2": "^3.8.3"
  },
  "devDependencies": {
    "code": "^5.2.4",
    "eslint": "4.x.x",
    "eslint-config-hapi": "11.x.x",
    "eslint-plugin-hapi": "4.x.x",
    "hpal-debug": "1.x.x",
    "lab": "^18.0.2",
    "lab-transform-preset-env": "^1.2.0",
    "nodemon": "^1.18.4"
  },
  "nodemonConfig": {
    "verbose": true,
    "ignore": [
      "node_modules",
      "performance-timing.csv"
    ],
    "delay": "2500"
  }
}
